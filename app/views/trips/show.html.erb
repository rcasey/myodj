<p id="notice"><%= notice %></p>

<p>
  <b>Date:</b>
  <%= @trip.date %>
</p>

<p>
  <b>Title:</b>
  <%= @trip.title %>
</p>

<p>
  <b>Destinations:</b>
  <%= @trip.destination.name if @trip.destination %>
</p>

<p>
  <b>Comment:</b>
  <%= @trip.comment %>
</p>

<%- @trip.partners.each do |p| %>
  <p>
    <%= p.name %>
  </p>
<%- end %>


<%= link_to 'Edit', edit_trip_path(@trip) %> |
<%= link_to 'Back', trips_path %>
